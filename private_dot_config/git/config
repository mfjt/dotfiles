	dd = difftool -d
	lol = log --graph --decorate --pretty=oneline --all --abbrev-commit
	pushf = push --force-with-lease --force-if-includes
[branch]
	sort = -committerdate
[column]
	ui = auto
[commit]
	verbose = true
[core]
	autocrlf = false
	editor = vim --nofork
	ignorecase = false
	quotepath = false
	safecrlf = true
[credential]
